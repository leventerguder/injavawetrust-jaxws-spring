<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://cxf.apache.org/jaxws 
	http://cxf.apache.org/schemas/jaxws.xsd">


	<jaxws:endpoint implementor="ws.HelloWorldImpl" address="/HelloWorldImplPort">
	</jaxws:endpoint>

	<jaxws:endpoint xmlns:sampleNameSpace="http://hello.example.com/" 
			address="/HelloWorldImplPortDetailed" serviceName="sampleNameSpace:HelloWorldImplService" 
			endpointName="sampleNameSpace:HelloWorldImplPortDetailed" implementor="ws.HelloWorldImpl"> 
	</jaxws:endpoint> 

	<jaxws:endpoint implementor="ws.ProductWebServiceImpl" address="/ProductWebServiceImplPort">
	</jaxws:endpoint>
	

</beans>